-- MAIN HUB --
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
-- Window
local MainWindow = OrionLib:MakeWindow({Name = "GarganScript || Version 1.2", HidePremium = false, SaveConfig = false, IntroEnabled = false})
-- Values
local Player = game.Players.LocalPlayer

-- Functions
 OrionLib:MakeNotification({
        Name = "Welcome!",
        Content = "Welcome, "..Player.Name.."! Thanks for using the GarganScript! Hope you have fun with it :)",
        Image = "rbxassetid://6523858422",
        Time = 4
    })

-- Tabs
local GenScriptsTab = MainWindow:MakeTab({
	Name = "General Scripts",
	Icon = "rbxassetid://3610245072",
	PremiumOnly = false
})
