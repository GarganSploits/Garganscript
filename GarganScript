	Name = "Universal Scripts",
	Icon = "rbxassetid://11560341145",
	PremiumOnly = false
})

local OtherScriptsTab = MainWindow:MakeTab({
	Name = "Other Scripts",
	Icon = "rbxassetid://3610247202",
	PremiumOnly = false
})
local Extras = MainWindow:MakeTab({
	Name = "Extras",
	Icon = "rbxassetid://3944677757",
	PremiumOnly = false
})


